@{
    ViewData["Title"] = "Editar";
}
<h1>Edição de contatos</h1>


<div>
    <form>
        <div class="form-group">
            <label for="nome">Nome</label>
            <input type="text" class="form-control" placeholder="Entre com o nome">

            <label for="email">Nome</label>
            <input type="text" class="form-control" placeholder="Entre com o email">

            <label for="telefone">Telefone</label>
            <input type="text" class="form-control" placeholder="Entre com o nome">
            
        </div>

        <button type="submit" class="btn btn-primary">Confirmar</button>
        <a type="submit" class="btn btn-primary" asp-controller="Contato" asp-action="Index" role="button">Cancelar</a>
        
    </form>

</div>